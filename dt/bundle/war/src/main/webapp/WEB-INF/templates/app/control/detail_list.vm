#foreach( $detail in $detailList) 	
    <div class="WB_feed_type SW_fun S_line2">
        <div class="WB_screen">
        </div>
        <div class="WB_feed_datail S_line2 clearfix">
            <div class="WB_face">
                <a class="W_face_radius" href="http://weibo.com/u/${detail.user.id}" title="${detail.user.screenName}"> 
					<img title="${detail.user.screenName}" alt="" width="50" height="50" src="${detail.user.avatarLarge}" />
                </a>
            </div>
            <div class="WB_detail">
                <div class="WB_info">
                    <a class="WB_name S_func1" title="${detail.user.screenName}" href="http://weibo.com/u/${detail.user.id}">${detail.user.screenName}</a>
                </div>
                <div class="WB_text">
                    ${detail.text}
                </div>
				#if(${detail.thumbnailPic})
				<ul class="WB_media_list clearfix">
					<li>
						<div class="chePicMin S_bg2 bigcursor">
							<img class="bigcursor" alt="" src="${detail.thumbnailPic}">
						</div>
					</li>
				</ul>
				#end
                <div class="WB_func clearfix">
                </div>
                <div class="WB_media_expand repeat S_line1 S_bg4" style="display: none;" />
            </div>
        </div>
    </div>
#end 